pipeline{
    agent any
   tools {
    maven 'M2_HOME'
    }
    stages{ 
        stage("git checkout"){
            steps{
                git 'https://github.com/mounikayemula/Jenkins-example.git/'
            }
        stage("Maven Build"){
            steps{
               sh "mvn clean package"
            }
        }
        
    }
}
